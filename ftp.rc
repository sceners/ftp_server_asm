	EDITTEXT IDC_LOGFILE, 20, 40, 160, 12, WS_BORDER | WS_TABSTOP | ES_AUTOHSCROLL

	DEFPUSHBUTTON "OK", IDC_OK, 150, 82, 40, 12

}


IDR_MENU MENU
{
